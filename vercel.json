{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "pages/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/pages/api/$1.js" },
    { "src": "/scripts/(.*)\\.js", "dest": "/scripts/$1.js" },
    { "src": "/styles/(.*)\\.css", "dest": "/styles/$1.css" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    {
      "src": "/Companies_and_candidates\\.json",
      "dest": "/Companies_and_candidates.json"
    },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
