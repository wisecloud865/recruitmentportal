{
  "version": 2,
  "routes": [
    { "src": "/api/updateField", "dest": "/api/updateField.js" },
    { "src": "/api/deleteCandidate", "dest": "/api/deleteCandidate.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "public": true,
  "headers": [
    {
      "source": "/scripts/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ]
}
